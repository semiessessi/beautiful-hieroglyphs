{
  "name": "Beautiful Hieroglyphs",
  "version": "0.1",
  "description": "Replace wikihiero images with beautiful, coloured equivalents.",
  "manifest_version": 3,
  "content_scripts": [{
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js":      ["assets/js/contentScript.js"]
  }],
  "icons": {
    "16": "assets/images/icon16.png",
    "32": "assets/images/icon32.png",
    "48": "assets/images/icon48.png",
    "128": "assets/images/icon128.png"
  }
}